#!/bin/bash
ARGS=
while [ $# -gt 0 ]
do
  case "$1" in
    #-MF) ARGS="$ARGS -Wc,\"$1 $2\""; shift;;
    -MF) ARGS="$ARGS -Wc,-MD,$2"; shift;;
    #-MM) ARGS="$ARGS -Wc,-MM";;
    #-MD) ARGS="$ARGS -Wc,-MD";;
    -MD) ;;
    -V*) ;;
    *) ARGS="$ARGS $1";;
  esac
  shift
done
echo "#!/bin/bash" > /tmp/qcc.$$
echo ${QNX_HOST}/usr/bin/QCC -V4.4.2,gcc_ntoarmv7le_gpp $ARGS > /tmp/qcc.$$
#echo /tmp/cmddump $ARGS > /tmp/qcc.$$
chmod +x /tmp/qcc.$$
/tmp/qcc.$$
rm -f /tmp/qcc.$$
